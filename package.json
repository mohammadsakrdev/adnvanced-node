{
  "name": "advanced-node",
  "version": "1.0.0",
  "description": "Advanced node concepts",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon app.js",
    "start-server": "node app.js",
